						}
						var showimage = $("#showimage")[0].src; 
	    	    	   $("#showimage1").attr('src',showimage);  
		  	           $("#house_photo_1").val(photo_min); 		
				    });
					$('#reup2').on('click',function(){
						var photo_min = $("#house_photo_0").val();
						if (null == photo_min || photo_min == ""){
							alert("请上传主图");
