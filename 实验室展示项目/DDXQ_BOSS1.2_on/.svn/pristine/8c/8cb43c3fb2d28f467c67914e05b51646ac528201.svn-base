<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd  
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	<bean id="weixinBase" class="com.jzsn.utils.weixin.WeiXinBase">
		<property name="App_ID" value="${weixin.AppID}" />
		<property name="App_Secret" value="${weixin.AppSecret}" />
		<property name="WeiXin_Code" value="${weixin.WeiXinCode}" />
		<property name="token" value="${weixin.token}" />
		<property name="welcomMsg" value="${weixin.welcomeMsg}" />
		<property name="welcomTip" value="${weixin.welcomeTip}" />
		<property name="API_Key" value="${weixin.APIKey}" />
		<property name="mch_Id" value="${weixin.mchId}" />
		<property name="menu" value="${weixin.menu}" />
	</bean>


</beans>