*{margin: 0; padding: 0; color: #333;list-style: none;}
body{font-family: 黑体; background: #f0f0f0; width: 100%; font-size: 0.7rem;}
html {font-size: 62.5%;}
a{text-decoration: underline; outline:none; color: #333;-webkit-tap-highlight-color:transparent;}
 input{border: 0; outline: none;}
 .body-limiam{width: 91.6%; height:7.7rem; margin: 0.5rem auto 1.2rem;
  padding: 0.9rem 4.2% 0; background: #fff;}
 .chongsu{text-indent: 2em; line-height: 1rem; font-size: 0.75rem;}
 .danxuan ul{list-style-type: none; width:100%;}
 /*.danxuan li{display: inline-block; margin: 0.6rem 0.5rem 0 0.5rem;}*/
 .danxuan li{width:20%; display:block; float:left }
 .qita{ float:right; margin: 2rem -5px 0.5rem 0; width: 50%;}
 .qita input{width: 20%;}
 .main{ width:93%; height:8rem; padding-top: 1.3rem; margin: 0 auto; text-indent: 2em;font-size: 0.6rem;}
 .bd spacing{height: 2.2rem; width: 100%;}
 
 
 .weui_btn {
  display: block;
  width: 95.8%;
  height: 2.2rem;
  margin: 0 auto;
  box-sizing: border-box;
  font-size: 0.75rem;
  text-align: center;
  text-decoration: none;
  color: #FFFFFF;
  line-height: 2.2rem;
  border-radius: 5px;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  overflow: hidden;
}
.weui_btn.weui_btn_inline {
  display: inline-block;
}
.weui_btn_default {
  background-color: #F7F7F7;
  color: #454545;
}
.weui_btn_default:not(.weui_btn_disabled):visited {
  color: #454545;
}
.weui_btn_default:not(.weui_btn_disabled):active {
  color: #A1A1A1;
  background-color: #DEDEDE;
}
.weui_btn_primary {
  background-color: #04BE02;
}
.weui_btn_primary:not(.weui_btn_disabled):visited {
  color: #FFFFFF;
}
.weui_btn_primary:not(.weui_btn_disabled):active {
  color: rgba(255, 255, 255, 0.4);
  background-color: #039702;
}

