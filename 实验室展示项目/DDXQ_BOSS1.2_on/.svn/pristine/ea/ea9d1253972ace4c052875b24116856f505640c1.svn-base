@charset "UTF-8";
.banner img{width: 100%;height: auto;}
.main{margin-top: -10px;padding: 20px 0 0 10px;}
.liaojie{color: #5FC2C9;}
.liaojie i{color: red;}
.liaojie p{text-indent: 2em;padding-bottom: 0.1rem;}
.ui-form-item {padding: 0;}
.ui-form-item input,.ui-form-item select, .ui-form-item textarea {width: 70%;-webkit-box-sizing: border-box;box-sizing: border-box;
    -webkit-appearance: none;border: 1px solid #5fc2c9;border-radius:5px;background: 0 0;padding: 0 10px;height: 30px;}
#tankuang2 .ui-form-item input,#tankuang2 .ui-form-item select,#tankuang2 .ui-form-item textarea{width: 60%;-webkit-box-sizing: border-box;box-sizing: border-box;
    -webkit-appearance: none;border: 1px solid #5fc2c9;border-radius:5px;background: 0 0;padding: 0 10px;margin-left: 15px;}
.fujia img{width: 100%;}
.fujia-neirong{width: 100%;text-indent: 2em;}
.button{width: 96%;height: 40px;border: 0;background: #5FC2C9;margin: 20px auto 30px;border-radius: 5px;color: #fff; padding: 0 2%;}
.shipin{height: 60px;}
.xingming{float: left;width: 25%;}
.huoqu{position: absolute;top: 7px;right: 5%;background: #5FC2C9;border-radius:0 5px 5px 0;height: 30px;width: 30%;
	text-align: center;color: #fff;line-height: 30px;font-size: 14px;}
.xiugai,.xiugai2{width: 40%; float: left;background: #5fc2c9;border: 0px solid #5fc2c9;outline: none; border-radius: 5px;height: 40px;color: #fff;}
.xiugai2{float: right;}
.zhezhao{width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000;opacity: 0.1;}
