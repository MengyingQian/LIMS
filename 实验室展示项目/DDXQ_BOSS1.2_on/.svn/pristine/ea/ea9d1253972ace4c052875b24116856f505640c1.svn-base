.touxiang-R-B img {position: absolute; width: 0.9rem;height: 0.9rem;border-radius: 5px;}
.uploader_input {position: absolute;z-index: 1;top: 0;left: 0;opacity: 0;width: 100%;height: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
.zhezhao{width: 100%; height: 200%; background: #000;
 	 position: absolute; z-index: 3; top:0; opacity: 0.3;}
.xingbie{width: 80%; height:0.8rem; margin: -10rem auto;left: 10%;background: #fff;
 border-radius: 5px; position: absolute;z-index: 4;}
 .xingbie-T{ margin: 0.1rem 0 0.1rem 5%; font-size: 0.2rem; font-weight: bold;}
 .xingbie-B {width: 100%; height: 1.5rem; line-height: 1.5rem;
 margin-right: 4px;}
