 html {font-size: 62.5%;/*10 ÷ 16 × 100% = 62.5%*/}
a{ text-decoration:none; outline:none; color: #333;-webkit-tap-highlight-color:transparent;}
*{padding: 0; margin: 0; font-size: 0.7rem; font-family: "黑体"; list-style-type: none;  color: #333;}
 body{background: #f0f0f0; width: 100%; font-family: 黑体; font-size: 0.7rem;
	font-family:Helvetica;padding-top: 40px;}   
input{border: 0; outline: none;}	
 .body-limiam{width: 100%; margin: 20px auto 9px auto; background: #fff; height: 2.1rem;position: relative;}
 .haoma-L{float: left; margin-top: 0.6rem; margin-left: 19px; font-family: arial;}
 .haoma-R input{float: left; height: 1.5rem; line-height: 1.5rem;
   margin-top: 0.4rem;margin-right: 4px;}
 .haoma-R2 input{float: left;height: 1.5rem; line-height: 1.5rem;
   margin-top: 0.4rem;margin-right: 4px; width: 35%;}
.ui-footer-btn{background: #f2f2f2;text-align: center;color: #b1dae0;font-weight: 500;}
.ui-btn-wrap{padding: 0;margin: 8px auto;height: 40px;width: 94%;border: 1px solid #b1dae0;
    border-radius: 5px;}
.ui-btn, .ui-btn-lg, .ui-btn-s {
    width: 100%;
    height: 40px;
    float: left;
    display: inline-block;
    position: relative;
    text-align: center;
    font-size: 16px;
    background-color: #fdfdfd;
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0.5,#fff),to(#fafafa));
    vertical-align: top;
    color: #5fc2c9;
    -webkit-box-sizing: border-box;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
   }