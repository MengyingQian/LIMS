@charset "UTF-8";
body{color: #fff;}
.page{background: -ms-linear-gradient(top, #081652,  #0d2864);
	background:-moz-linear-gradient(top,#081652,#0d2864);
	background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#081652), to(#0d2864));
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#081652), to(#0d2864));
	background: -webkit-linear-gradient(top, #081652, #0d2864);
	background: -o-linear-gradient(top, #081652, #0d2864);}
.center{
	background: -ms-linear-gradient(top, #0d2864,  #215498);
	background:-moz-linear-gradient(top,#0d2864,  #215498);
	background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#0d2864), to(#215498));
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#0d2864), to(#215498));
	background: -webkit-linear-gradient(top, #0d2864,  #215498);
	background: -o-linear-gradient(top, #0d2864,  #215498);
	height: auto;padding: 0.2rem 0 0.1rem;}
.bgjianbian2{
	background: -ms-linear-gradient(top, #215498, #324fa2);
	background:-moz-linear-gradient(top, #215498, #324fa2);
	background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#215498), to(#324fa2));
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#215498), to(#324fa2));
	background: -webkit-linear-gradient(top, #215498, #324fa2);
	background: -o-linear-gradient(top, #215498, #324fa2);
	height: auto;}
.bgjianbian3{
	background: -ms-linear-gradient(top, #215498, #081652);
	background:-moz-linear-gradient(top, #215498, #081652);
	background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#215498), to(#081652));
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#215498), to(#081652));
	background: -webkit-linear-gradient(top, #215498, #081652);
	background: -o-linear-gradient(top, #215498, #081652);
	height: auto;}
.page img{width: 100%;}
.page ul{position: absolute;list-style: none;text-align: justify;top: 1.9rem;left: 0;width: 100%;}
.page ul li{display: inline-block;width: 32.3%;line-height:20px;border-right: 1px solid #fff;}
.page ul li div{text-align: center;height: 20px;}
.page ul li:last-child{border-right: 0;}
.xunzhang{border-bottom: 15px solid #123270;border-top: 15px solid #123270;border-right: 6px solid transparent;
	position: absolute;top: 0px; right: -6px;}
.main{width: 70%;margin: -120px auto 20px;background: rgba(255,255,255,0.3);border-radius: 5px;padding: 10px;}
.xingming{width: 100%;height: auto;line-height: 25px;font-size: 0.12rem;}
.baoming {width: 74%;background: rgba(255,255,255,0.3);height: 0.3rem;color: #fff;display: block;margin: 10px auto;
    font-size: 0.15rem;line-height: 0.3rem;text-align: center;border-radius: 5px;}
.hd_bg{background: url(/view/weixin/jsbian/qika/fengcai/img/bg_02-2.png) no-repeat;background-size: 100% 100%;padding: 0.1rem 0 1.1rem; font-size: 0.13rem;margin-top: -0.11rem;}
.huodongjianjie{height: auto;}
.hd_bg p,.bgjianbian2 p{margin: 0 0 0.1rem;}
.huodongjianjie .jianjie,.hd_bg p em,.bgjianbian2 p em{font-size:0.15rem;font-style:normal;padding-left: 0.1rem;background: #123270;position: relative; height: 30px;
	line-height: 30px;width: 82px;display: block;float: left;color: #fff;}
.zhengwen{padding: 0 0 0 20px;line-height:0.2rem;font-size:0.13rem;}
.zhengwen .jieshao{text-indent: 2em;width: 96%;}
.zhengwen .jieduan{width: 100%;height: auto;padding-bottom: 0.1rem;}
.zhengwen .jieduan .jibie{width: 24%;float: left;}
.zhengwen .jieduan .neirong{width: 73%;float: left; }
.zhengwen p{text-indent: 2em;}
.bmgz{padding: 0.1rem 0.1rem 0 0.9rem;}
.bmgz p{line-height: 0.16rem;letter-spacing: -1px;border: solid 0.01rem #17b59d;border-radius: 0.05rem;margin-bottom: 0;padding: 0.05rem;line-height: 0.18rem;}
.jxsz{padding: 30px 0 0 6%;margin-top: 20px;}
.jxsz span{display: inline-block;margin: 0 10px 0 0;}
.yuandian{width: 10px;height: 10px;border-radius: 5px;background: #fff;}
.jxsz p{padding: 0 0 0 18%;margin: -10px 0 0;}
.jxsz .fuze{padding: 0 0 0 28%;}
.erweima img {width: 100%;}
.xian{height: 0.14rem;background: url(/view/weixin/jsbian/qika/fengcai/img/jiantou2_03.png) no-repeat center;width: 100%;background-size: 12% 100%;margin-top: -1px;}
